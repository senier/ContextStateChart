with "contrib/SXML/SXML";

project Build
is
   for Create_Missing_Dirs use "True";
   for Source_Dirs use ("src");
   for Object_Dir use "obj";

   package Compiler is
      for Switches ("Ada") use ("-gnatef", "-gnatU");
   end Compiler;

   package Builder is
      for Global_Configuration_Pragmas use "scsc.adc";
   end Builder;
end Build;
